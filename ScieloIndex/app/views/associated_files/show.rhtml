<% for column in AssociatedFile.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%=h @record.send(column.name) %>
</p>
<% end %>
<div id="buttonsNavigator">
<%= link_to 'Edit', {:action => 'edit', :id => @record}, :class => 'cssbutton' %> |
<%= link_to 'Back', {:action => 'list'}, :class => 'cssbutton' %>
</div>